# Stable Diffusion Configuration Template
# Copy to sd_config.env and customize

# ===== GROK API =====
GROK_API_KEY=xai-YOUR_KEY_HERE
GROK_BASE_URL=https://api.x.ai/v1

# ===== STABLE DIFFUSION =====
STABLE_DIFFUSION_URL=http://localhost:7860

# ===== MODEL SETTINGS =====
SD_MODEL_CHECKPOINT=AetherCrown.safetensors
SD_MODEL_FALLBACK=Sable_room.safetensors

# ===== SAMPLING =====
SAMPLER_NAME=Euler A Automatic
sampling_steps=20
CFG_SCALE=7.0

# ===== HIGH-RES FIX =====
ENABLE_HIRES_FIX=false
HR_UPSCALER=R-ESRGAN 4x+ Anime6B
HR_STEPS=20
DENOISING_STRENGTH=0.4
HR_SCALE=2.0

# ===== IMAGE SETTINGS =====
WIDTH=512
HEIGHT=768
BATCH_SIZE=1
N_ITER=1

# ===== TIER CONFIGURATION =====
KIDS_DAILY_LIMIT=3
STANDARD_DAILY_LIMIT=10
PREMIUM_DAILY_LIMIT=999

# ===== TIER SPECIFIC SETTINGS =====
KIDS_STEPS=20
KIDS_HR_STEPS=10
KIDS_DENOISING=0.3

STANDARD_STEPS=20
STANDARD_HR_STEPS=20
STANDARD_DENOISING=0.4

PREMIUM_STEPS=20
PREMIUM_HR_STEPS=30
PREMIUM_DENOISING=0.5
